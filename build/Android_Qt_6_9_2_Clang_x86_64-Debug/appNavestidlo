#!/bin/sh

/home/jan/Qt/6.9.2/gcc_64/libexec/qt-android-runner.py \
    --adb "/home/jan/Android/Sdk/platform-tools/adb" \
    --build-path "/home/jan/Navestidlo/build/Android_Qt_6_9_2_Clang_x86_64-Debug/android-build-appNavestidlo" \
    --apk "/home/jan/Navestidlo/build/Android_Qt_6_9_2_Clang_x86_64-Debug/android-build-appNavestidlo/appNavestidlo.apk" \
    $@

